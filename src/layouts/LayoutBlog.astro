---
import Layout from "./Layout.astro";

interface Props {
    title : string
}

const { title} = Astro.props 
---

<Layout title={title}>
    <main class="mx-auto max-w-6xl px-6 pt-28 md:px-12 xl:pt-20 xl:px-0">
        <h2 class="text-3xl font-semibold">{title}</h2>
        <slot/>
    </main>
</Layout>